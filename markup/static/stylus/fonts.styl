body {
  font-size: 16px;
  line-height: 19px;
  font-weight 400
  font-family: sans-serif;
  color: #000000;
  font-style: normal;
}

*::-webkit-input-placeholder {
  color #808080
  font-size: 19px;
  line-height: 23px;
  font-weight 400
  font-family: 'SFUIDisplay', sans-serif;
  
  opacity: 1;
}

